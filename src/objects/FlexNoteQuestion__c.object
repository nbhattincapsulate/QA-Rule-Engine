    <validationRules>
        <fullName>Enforce_Unique_FlexNote_Question_Code</fullName>
        <active>true</active>
        <description>Prohibit creation of duplicate FlexNote Question codes.  This is important for integration and Open311.</description>
        <errorConditionFormula>IF(ISNEW(), IF( UPPER(Name) == UPPER(VLOOKUP( $ObjectType.FlexNoteQuestion__c.Fields.Name , $ObjectType.FlexNoteQuestion__c.Fields.Name , Name)),true , false),AND(ISCHANGED(Name), IF( UPPER(Name) == UPPER(VLOOKUP($ObjectType.FlexNoteQuestion__c.Fields.Name,$ObjectType.FlexNoteQuestion__c.Fields.Name, Name)),true,false)))</errorConditionFormula>
        <errorMessage>FlexNote Question code cannot have duplicate value.</errorMessage>
    </validationRules>
