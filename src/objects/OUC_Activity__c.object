        <customTabListAdditionalFields>Status__c</customTabListAdditionalFields>
        <customTabListAdditionalFields>Final_Outcome__c</customTabListAdditionalFields>
        <customTabListAdditionalFields>Completion_Date__c</customTabListAdditionalFields>
        <customTabListAdditionalFields>Citizen_Email_On_Complete__c</customTabListAdditionalFields>
        <searchResultsAdditionalFields>Status__c</searchResultsAdditionalFields>
        <searchResultsAdditionalFields>SR_Status__c</searchResultsAdditionalFields>
        <searchResultsAdditionalFields>Case_Number__c</searchResultsAdditionalFields>
    </searchLayouts>
    <sharingModel>ReadWrite</sharingModel>
    <validationRules>
        <fullName>Close_SR_Outcome_External_Comments</fullName>
        <active>true</active>
        <description>Per Dilshad: When Outcomes are chosen (or via Open311) that will Close the SR, External Comments cannot be null.</description>
        <errorConditionFormula>AND ( 
OR( CONTAINS ( Final_Outcome__c , &apos;Close SR&apos;), 
CONTAINS ( Final_Outcome__c , &apos;Closed,&apos;) ), 
ISPICKVAL(Status__c, &apos;Completed&apos;), 
OR( External_Comments__c = &apos;&apos;, 
ISNULL ( External_Comments__c) 
) 
)</errorConditionFormula>
        <errorDisplayField>External_Comments__c</errorDisplayField>
        <errorMessage>The selected outcome will close the service request. External Comments are required when selecting such an outcome.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Lock_OUC_Activity_when_Case_Closed</fullName>
        <active>true</active>
        <description>Per Dilshad: Prevent edits to OUC Activities once the service request is closed.</description>
        <errorConditionFormula>Case__r.IsClosed</errorConditionFormula>
        <errorMessage>No edits can be made to OUC Activities when service request is closed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Test_External_Comments_mandatory</fullName>
        <active>true</active>
        <description>Per Dilshad: When Outcomes are chosen (or via Open311) that will Close the SR, External Comments cannot be null.</description>
        <errorConditionFormula>AND(OR(CONTAINS (Final_Outcome__c, &apos;Close SR&apos;), 
CONTAINS ( Final_Outcome__c , &apos;Closed,&apos;)), ISBLANK( External_Comments__c ), Allowable_Outcomes__r.Can_Close_SR__c = True )</errorConditionFormula>
        <errorDisplayField>External_Comments__c</errorDisplayField>
        <errorMessage>External comments are mandatory</errorMessage>
    </validationRules>
